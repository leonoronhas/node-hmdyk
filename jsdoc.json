{
    "source": {
        "include": ["./controllers", "./models", "./routes"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/docs)"
    },
    "plugins": [
        "plugins/marksown"
    ],
    "opts":{
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    }
}